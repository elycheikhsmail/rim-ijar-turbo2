{
  "name": "rim-ijar-turbo",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "build": "pnpm --filter ./apps/rim-ebay  run build",
    "generate": "pnpm --filter ./apps/rim-ebay  run generate && pnpm --filter ./packages/mytypes  run generate ",
    "seed": "turbo run seed",
    "build:only": "pnpm  --filter ./apps/rim-ebay run build:only",
    "dev": "pnpm --filter ./apps/rim-ebay dev",

    "start": "pnpm --filter ./apps/rim-ebay start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types --cache=local:r "
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.1.6",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "turbo": "^2.4.4",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.6.3",
  "engines": {
    "node": ">=22"
  }
}
