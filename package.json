{
  "name": "rim-ijar-turbo",
  "private": true,
  "scripts": {
    "prepare": "husky",
    "generate": "pnpm --filter ./apps/rim-ebay  run generate && pnpm --filter ./packages/mytypes  run generate ",
    "seed": "turbo run seed",
    "build:only": "pnpm  --filter ./apps/rim-ebay run build:only",
    "dev": "pnpm --filter ./apps/rim-ebay dev",
    "start": "pnpm --filter ./apps/rim-ebay start",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "pnpm  --filter ./packages/ui check-types "
  },
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "better-sqlite3": "^12.2.0",
    "next": "15.3.3",
    "react": "19.1.0"
  },
   
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.6",
    "husky": "^9.1.7",
    "prettier": "^3.5.3",
    "prisma": "^6.13.0",
    "typescript": "5.8.2"
  },
  "packageManager": "pnpm@10.6.3",
  "engines": {
    "node": ">=22"
  },
  "pnpm": {
      "overrides": {
      "@types/react": "^18.2.0",
      "@types/react-dom": "^18.2.0"
    },
    "onlyBuiltDependencies": [
      "@prisma/client",
      "@prisma/engines",
      "bcrypt",
      "better-sqlite3",
      "core-js",
      "esbuild",
      "prisma",
      "sharp",
      "unrs-resolver"
    ]
  }
}
